        "    f.write('CREATE TABLE IF NOT EXISTS doctors (\\n')\n",
        "    f.write('    doctor_id INTEGER PRIMARY KEY,\\n')\n",
        "    f.write('    name TEXT NOT NULL,\\n')\n",
        "    f.write('    specialization TEXT\\n')\n",
