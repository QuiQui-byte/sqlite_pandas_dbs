      },
      "source": [
        "import sqlite3\n",
        "import pandas as pd\n",
        "import os\n",
        "\n",
        "db_path = os.path.join('health-sqlite-lite', 'clinic_simple.db')\n",
        "combined_analysis_sql_path = os.path.join('health-sqlite-lite', 'sql', 'combined_analysis.sql')\n",
        "\n",
        "# Read the SQL query from combined_analysis.sql\n",
        "with open(combined_analysis_sql_path, 'r') as f:\n",
        "    sql_query_combined = f.read()\n",
        "\n",
        "# Connect to the database and execute the query\n",
        "conn = sqlite3.connect(db_path)\n",
        "df_combined_analysis = pd.read_sql_query(sql_query_combined, conn)\n",
        "conn.close()\n",
        "\n",
        "# Print the resulting DataFrame\n",
        "print(\"Combined Analysis Results:\")\n",
        "print(df_combined_analysis)"
      ],
