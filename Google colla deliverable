    {
      "cell_type": "code",
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "abdb130a",
        "outputId": "c8e1a7d0-767a-4a1e-e85a-8f19caf118d0"
      },
      "source": [
        "import sqlite3\n",
        "import pandas as pd\n",
        "import os\n",
        "\n",
        "# Create import_doctors_csv.py\n",
        "with open('health-sqlite-lite/src/import_doctors_csv.py', 'w') as f:\n",
        "    f.write(\"\"\"import sqlite3\n",
        "import pandas as pd\n",
        "import os\n",
        "\n",
        "def import_csv_to_db(db_path, csv_path, table_name):\n",
        "    conn = sqlite3.connect(db_path)\n",
        "    try:\n",
        "        df = pd.read_csv(csv_path)\n",
        "        df.to_sql(table_name, conn, if_exists='append', index=False)\n",
        "        print(f\"Data from {csv_path} imported to table {table_name} in {db_path}\")\n",
        "    except Exception as e:\n",
        "        print(f\"Error importing data: {e}\")\n",
        "    finally:\n",
        "        conn.close()\n",
        "\n",
        "if __name__ == '__main__':\n",
        "    project_root = 'health-sqlite-lite'\n",
        "    db_path = os.path.join(project_root, 'clinic_simple.db')\n",
        "    csv_path = os.path.join(project_root, 'data', 'doctors.csv')\n",
        "    table_name = 'doctors'\n",
        "    import_csv_to_db(db_path, csv_path, table_name)\n",
        "\"\"\")\n",
        "\n",
        "print(\"Python script import_doctors_csv.py created.\")"
      ],
      "execution_count": null,
      "outputs": [
        {
          "output_type": "stream",
          "name": "stdout",
          "text": [
            "Python script import_doctors_csv.py created.\n"
          ]
        }
      ]
