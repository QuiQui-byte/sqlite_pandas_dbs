    {
      "cell_type": "code",
      "source": [
        "import os\n",
        "\n",
        "# Create directories\n",
        "os.makedirs('health-sqlite-lite/data', exist_ok=True)\n",
        "os.makedirs('health-sqlite-lite/sql', exist_ok=True)\n",
        "os.makedirs('health-sqlite-lite/src', exist_ok=True)\n",
        "\n",
        "# Create placeholder files\n",
        "!touch health-sqlite-lite/sql/schema.sql\n",
        "!touch health-sqlite-lite/sql/analysis.sql\n",
        "!touch health-sqlite-lite/requirements.txt\n",
        "!touch health-sqlite-lite/README.md\n",
        "\n",
        "# Placeholder for patients.csv (you might want to upload your actual file later)\n",
        "with open('health-sqlite-lite/data/patients.csv', 'w') as f:\n",
        "    f.write('patient_id,name,age,gender\\n')\n",
        "    f.write('1,John Doe,30,Male\\n')\n",
        "    f.write('2,Jane Smith,25,Female\\n')\n",
        "\n",
        "# Content for schema.sql\n",
        "with open('health-sqlite-lite/sql/schema.sql', 'w') as f:\n",
        "    f.write('-- schema.sql\\n')\n",
        "    f.write('CREATE TABLE IF NOT EXISTS patients (\\n')\n",
        "    f.write('    patient_id INTEGER PRIMARY KEY,\\n')\n",
        "    f.write('    name TEXT NOT NULL,\\n')\n",
        "    f.write('    age INTEGER,\\n')\n",
        "    f.write('    gender TEXT\\n')\n",
        "    f.write(');\\n')\n",
        "\n",
        "# Content for analysis.sql (placeholder)\n",
        "with open('health-sqlite-lite/sql/analysis.sql', 'w') as f:\n",
        "    f.write('-- analysis.sql\\n')\n",
        "    f.write('SELECT gender, AVG(age) FROM patients GROUP BY gender;\\n')\n",
        "\n",
        "print(\"Project structure and placeholder files created successfully.\")\n",
        "\n",
        "# Create create_db.py\n",
        "with open('health-sqlite-lite/src/create_db.py', 'w') as f:\n",
        "    f.write(\"\"\"import sqlite3\n",
        "import os\n",
        "\n",
        "def create_database(db_path, schema_sql_path):\n",
        "    conn = sqlite3.connect(db_path)\n",
        "    cursor = conn.cursor()\n",
        "\n",
        "    with open(schema_sql_path, 'r') as f:\n",
        "        sql_script = f.read()\n",
        "    cursor.executescript(sql_script)\n",
        "\n",
        "    conn.commit()\n",
        "    conn.close()\n",
        "    print(f\"Database created and schema applied at {db_path}\")\n",
        "\n",
        "if __name__ == '__main__':\n",
        "    project_root = 'health-sqlite-lite'\n",
        "    db_path = os.path.join(project_root, 'clinic_simple.db')\n",
        "    schema_sql_path = os.path.join(project_root, 'sql', 'schema.sql')\n",
        "    create_database(db_path, schema_sql_path)\n",
        "\"\"\")\n",
        "\n",
        "# Create import_csv.py\n",
        "with open('health-sqlite-lite/src/import_csv.py', 'w') as f:\n",
        "    f.write(\"\"\"import sqlite3\n",
        "import pandas as pd\n",
        "import os\n",
        "\n",
        "def import_csv_to_db(db_path, csv_path, table_name):\n",
        "    conn = sqlite3.connect(db_path)\n",
        "    try:\n",
        "        df = pd.read_csv(csv_path)\n",
        "        df.to_sql(table_name, conn, if_exists='append', index=False)\n",
        "        print(f\"Data from {csv_path} imported to table {table_name} in {db_path}\")\n",
        "    except Exception as e:\n",
        "        print(f\"Error importing data: {e}\")\n",
        "    finally:\n",
        "        conn.close()\n",
        "\n",
        "if __name__ == '__main__':\n",
        "    project_root = 'health-sqlite-lite'\n",
        "    db_path = os.path.join(project_root, 'clinic_simple.db')\n",
        "    csv_path = os.path.join(project_root, 'data', 'patients.csv')\n",
        "    table_name = 'patients'\n",
        "    import_csv_to_db(db_path, csv_path, table_name)\n",
        "\"\"\")\n",
        "\n",
        "print(\"Python scripts create_db.py and import_csv.py created.\")\n",
        "\n",
        "# Run the scripts\n",
        "print(\"\\n--- Running create_db.py ---\")\n",
        "!python health-sqlite-lite/src/create_db.py\n",
        "\n",
        "print(\"\\n--- Running import_csv.py ---\")\n",
        "!python health-sqlite-lite/src/import_csv.py\n",
        "\n",
        "print(\"\\nDatabase setup and data import complete. You can now query clinic_simple.db\")"
      ],
      "metadata": {
