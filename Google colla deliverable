    },
    {
      "cell_type": "code",
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "646debd6",
        "outputId": "95ba8269-00be-4f03-d981-1d497bd5d295"
      },
      "source": [
        "lab_results_analysis_sql_path = os.path.join('health-sqlite-lite', 'sql', 'lab_results_analysis.sql')\n",
        "\n",
        "with open(lab_results_analysis_sql_path, 'w') as f:\n",
        "    f.write('-- lab_results_analysis.sql\\n')\n",
        "    f.write('SELECT test_name, COUNT(result_id) AS num_results\\n')\n",
        "    f.write('FROM lab_results\\n')\n",
        "    f.write('GROUP BY test_name\\n')\n",
        "    f.write('ORDER BY num_results DESC;\\n')\n",
        "\n",
        "print(f\"Created {lab_results_analysis_sql_path} with the analysis query.\")"
      ],
      "execution_count": null,
