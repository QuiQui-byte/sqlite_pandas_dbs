      },
      "source": [
        "import sqlite3\n",
        "import matplotlib.pyplot as plt\n",
        "import seaborn as sns\n",
        "\n",
        "db_path = os.path.join('health-sqlite-lite', 'clinic_simple.db')\n",
        "medical_records_analysis_sql_path = os.path.join('health-sqlite-lite', 'sql', 'medical_records_analysis.sql')\n",
        "\n",
        "# Read the SQL query from medical_records_analysis.sql\n",
        "with open(medical_records_analysis_sql_path, 'r') as f:\n",
        "    sql_query_medical_records = f.read()\n",
        "\n",
        "# Connect to the database and execute the query\n",
        "conn = sqlite3.connect(db_path)\n",
        "df_medical_records_analysis = pd.read_sql_query(sql_query_medical_records, conn)\n",
        "conn.close()\n",
        "\n",
        "# Print the resulting DataFrame\n",
        "print(\"Analysis results for medical records diagnosis frequency:\")\n",
        "print(df_medical_records_analysis)\n",
        "\n",
        "# Create a bar plot\n",
        "plt.figure(figsize=(8, 5))\n",
        "sns.barplot(x='diagnosis', y='num_records', data=df_medical_records_analysis, palette='viridis', hue='diagnosis', legend=False)\n",
        "plt.title('Diagnosis Frequency')\n",
        "plt.xlabel('Diagnosis')\n",
        "plt.ylabel('Number of Records')\n",
        "plt.show()"
      ],
      "execution_count": null,
      "outputs": [
        {
          "output_type": "stream",
          "name": "stdout",
