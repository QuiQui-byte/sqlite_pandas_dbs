      "source": [
        "import matplotlib.pyplot as plt\n",
        "import seaborn as sns\n",
        "import pandas as pd\n",
        "\n",
        "# Prepare data for plotting: Melt the DataFrame to long format\n",
        "df_melted = df_combined_analysis.melt(id_vars=['patient_id', 'patient_name', 'age', 'gender', 'doctor_name', 'specialization', 'diagnosis'],\n",
        "                                      var_name='Activity Type',\n",
        "                                      value_name='Count')\n",
        "\n",
        "# Filter only the count columns for visualization\n",
        "df_melted_counts = df_melted[df_melted['Activity Type'].isin(['num_appointments', 'num_prescriptions', 'num_medical_records', 'num_lab_results'])]\n",
        "\n",
        "# Rename activity types for better readability\n",
        "df_melted_counts['Activity Type'] = df_melted_counts['Activity Type'].replace({\n",
        "    'num_appointments': 'Appointments',\n",
        "    'num_prescriptions': 'Prescriptions',\n",
        "    'num_medical_records': 'Medical Records',\n",
        "    'num_lab_results': 'Lab Results'\n",
        "})\n",
        "\n",
        "\n",
        "# Create the grouped bar chart\n",
        "plt.figure(figsize=(12, 6))\n",
        "sns.barplot(x='patient_name', y='Count', hue='Activity Type', data=df_melted_counts, palette='viridis')\n",
        "plt.title('Medical Activity Counts per Patient')\n",
        "plt.xlabel('Patient Name')\n",
        "plt.ylabel('Count')\n",
        "plt.xticks(rotation=45, ha='right')\n",
        "plt.legend(title='Activity Type')\n",
        "plt.tight_layout()\n",
        "plt.show()"
      ],
